<?xml version="1.0" encoding="utf-8"?>
<com.aries.ui.view.title.TitleBarView
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:id="@+id/titleBar"
    android:layout_width="match_parent"
    android:layout_height="48dp"
    android:background="@color/colorTitle"
    app:title_dividerColor="@color/colorLineGray"
    app:title_dividerHeight="@dimen/dp_line_size"
    app:title_immersible="true"
    app:title_leftTextColor="@color/colorAccent"
    app:title_leftTextDrawable="@drawable/ic_arrow_left"
    app:title_leftTextSize="14dp"
    app:title_outPadding="12dp"
    app:title_titleSubTextColor="@color/colorTextBlack"
    app:title_rightTextColor="@color/colorAccent"
    app:title_rightTextSize="14dp"
    app:title_titleMainTextColor="@color/colorAccent"
    app:title_actionTextColor="@color/colorAccent"
    app:title_titleMainTextSize="18dp"/>



